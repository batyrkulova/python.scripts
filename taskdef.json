{
  "family": "static-site",
  "networkMode": "bridge",
  "containerDefinitions": [
    {
      "name": "static-site",
      "image": "elsuloda/my_app:v1-amd64",
      "cpu": 256,
      "memory": 512,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": ["EC2"],
  "cpu": "256",
  "memory": "512"
}
